<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/black_222e3b</item>
        <item name="colorPrimaryDark">@color/black_222e3b</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="MyButtons" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorButtonNormal">@color/blue_2060e5</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="BrandedSwitch.control" parent="Theme.AppCompat.Light">
        <!-- active thumb & track color (30% transparency) -->
        <item name="colorControlActivated">@color/green_00c37c</item>
        <item name="colorSwitchThumbNormal">@color/gray_e6ebf4</item>
    </style>



</resources>
